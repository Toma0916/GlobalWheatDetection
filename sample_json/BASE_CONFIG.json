{
    "debug": true,
    "general": {
        "output_dirname": "sample",
        "seed": 42,
        "model_save_interval": 5,
        "valid_image_save_interval": 5,
        "bbox_filter": {
            "max_bbox_size": 10000000,
            "min_bbox_size": 0
        },
        "train_valid_split": {
            "name": "random"
        }
    },
    "model": {
        "name": "faster_rcnn", 
        "config": {
            "backbone": "fasterrcnn_resnet50_fpn",
            "class_num": 2,
            "pretrained": true
        }
    },
    "train": {
        "batch_size": 8,
        "epochs": 10,
        "random_annotation": {
            "drop": {
                "p": 0.0
            },
            "vibration": {
                "max_ratio": 0.0
            }
        },
        "sampler": {
            "name": ""
        },
        "optimizer": {
            "name": "adam",
            "config": {
                "lr": 0.0001,
                "betas": {
                    "tuple": {
                        "b1": 0.5,
                        "b2": 0.999
                    }
                },
                "eps": 1e-8
            }
        },
        "scheduler": {
            "name": "Step",
            "config": {
                "step_size": 10,
                "gamma": 0.1,
                "last_epoch": -1
            }
        },
        "augment": {
            "test_time_augment": false
        }
    },
    "valid": {
        "apply_bbox_filter": false,
        "confidence_filter": {
            "min_confidence": 0.0
        },
        "post_processor": {
            "name": ""
        }
    }
}