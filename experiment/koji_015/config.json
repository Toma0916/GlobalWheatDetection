{
    "debug": false,
    "general": {
        "output_dirname": "koji/exp_015_effdet",
        "experiment_name": "manhattan",
        "seed": 99,
        "model_save_interval": 3,
        "valid_image_save_interval": 1,
        "bbox_filter": {
            "max_bbox_size": 160000,
            "min_bbox_size": 90
        }
    },
    "model": {
        "name": "efficient_det", 
        "config": {
            "pretrained_path": "/home/minoda/git/GlobalWheatDetection/weights/effdet/efficientdet_d5-ef44aea8.pth",
            "image_size": 512
        }
    },
    "train": {
        "batch_size": 2,
        "epochs": 100,
        "random_annotation": {
            "drop": {
                "p": 0.0
            },
            "vibration": {
                "max_ratio": 0.0
            }
        },
        "sampler": {
            "name": "balance_sources",
            "config": {
                "strength": 1000
            }
        },
        "optimizer": {
            "name": "adam",
            "config": {
                "lr": 0.0001,
                "betas": {
                    "tuple": {
                        "b1": 0.5,
                        "b2": 0.999
                    }
                },
                "eps": 1e-8
            }
        },
        "scheduler": {
            "name": "Step",
            "config": {
                "step_size": 10,
                "gamma": 0.1,
                "last_epoch": -1
            }
        },
        "augment": {
            "horizontal_flip": {
                "p": 0.5
            },
            "vertical_flip": {
                "p": 0.5
            },
            "blur": {
                "p": 0.5
            },
            "motion_blur": {
                "p": 0.5
            },
            "random_brightness": {
                "p": 0.5,
                "limit": 0.6
            },
            "random_contrast": {
                "p": 0.5,
                "limit": {
                    "tuple": {
                        "1": 0.7,
                        "2": 0.9
                    }
                }
            },
            "test_time_augment": false
        }
    }
}