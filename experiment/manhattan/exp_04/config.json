{
    "debug": false,
    "general": {
        "output_dirname": "manhattan/exp_04",
        "experiment_name": "manhattan",
        "seed": 42,
        "model_save_interval": 5,
        "valid_image_save_interval": 2,
        "bbox_filter": {
            "max_bbox_size": 100000,
            "min_bbox_size": 100
        }
    },
    "model": {
        "name": "efficient_det", 
        "config": {
            "pretrained_path": "/home/minoda/git/GlobalWheatDetection/weights/effdet/efficientdet_d5-ef44aea8.pth",
            "image_size": 512
        }
    },
    "train": {
        "batch_size": 8,
        "epochs": 51,
        "optimizer": {
            "name": "adam",
            "config": {
                "lr": 0.0001,
                "betas": {
                    "tuple": {
                        "b1": 0.5,
                        "b2": 0.999
                    }
                },
                "eps": 1e-8
            }
        },
        "scheduler": {
            "name": "Step",
            "config": {
                "step_size": 10,
                "gamma": 0.1,
                "last_epoch": -1
            }
        },
        "augment": {
            "horizontal_flip": {
                "p": 0.5
            },
            "vertical_flip": {
                "p": 0.5
            },
            "random_rotate_90": {
                "p": 0.5
            },
            "hsv": {
                "p": 0.5
            },
            "random_contrast": {
                "p": 0.5
            },
            "random_brightness": {
                "p": 0.5
            },
            "mosaic": {
                "p": 0.5
            }, 
            "mixup": {
                "p": 0.5
            },
            "random_sized_bbox_safe_crop": {
                "height": 1024,
                "width": 1024,
                "p": 0.5
            },
            "test_time_augment": false
        }
    },
    "valid": {
        "apply_bbox_filter": true,
        "confidence_filter": {
            "min_confidence": 0.2
        },
        "post_processor": {
            "name": "wbf",
            "config": {
                "threshold": 0.6
            }
        }
    }
}