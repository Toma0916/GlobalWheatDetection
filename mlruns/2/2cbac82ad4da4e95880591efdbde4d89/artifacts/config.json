{
    "debug": false,
    "general": {
        "output_dirname": "koji/exp_016",
        "experiment_name": "manhattan",
        "seed": 42,
        "model_save_interval": 2,
        "bbox_filter": {
            "max_bbox_size": 100000,
            "min_bbox_size": 100
        },
        "prefix": "gn",
        "valid_image_save_interval": 5,
        "train_valid_split": {
            "name": "random",
            "config": {}
        }
    },
    "model": {
        "name": "faster_rcnn",
        "config": {
            "backbone": "resnext50_32x4d",
            "class_num": 2,
            "pretrained": false
        },
        "prefix": "md"
    },
    "train": {
        "batch_size": 8,
        "epochs": 51,
        "optimizer": {
            "name": "sgd",
            "config": {
                "lr": 0.005,
                "momentum": 0.9,
                "weight_decay": 0.0005
            },
            "eps": 1e-08
        },
        "scheduler": {
            "name": "Step",
            "config": {
                "step_size": 10,
                "gamma": 0.1,
                "last_epoch": -1
            }
        },
        "augment": {
            "horizontal_flip": {
                "p": 0.5
            },
            "vertical_flip": {
                "p": 0.5
            },
            "blur": {
                "p": 0.5
            },
            "motion_blur": {
                "p": 0.5
            },
            "random_brightness": {
                "p": 0.3,
                "limit": 0.2
            },
            "random_contrast": {
                "p": 0.3,
                "limit": [
                    0.5,
                    0.7
                ]
            },
            "prefix": "ag",
            "random_rotate_90": {
                "p": 0.0
            },
            "median_blur": {
                "p": 0.0
            },
            "gaussian_blur": {
                "p": 0.0
            },
            "optical_distortion": {
                "p": 0.0
            },
            "grid_distortion": {
                "p": 0.0
            },
            "elastic_transform": {
                "p": 0.0
            },
            "clahe": {
                "p": 0.0
            },
            "channel_shuffle": {
                "p": 0.0
            },
            "random_gamma": {
                "p": 0.0
            },
            "hsv": {
                "p": 0.0
            },
            "rgb_shift": {
                "p": 0.0
            },
            "random_sized_bbox_safe_crop": {
                "p": 0.0,
                "height": 1024,
                "width": 1024
            },
            "gauss_noise": {
                "p": 0.0
            },
            "cutout": {
                "p": 0.0
            },
            "mosaic": {
                "p": 0.0
            },
            "cutmix": {
                "p": 0.0
            },
            "mixup": {
                "p": 0.0
            },
            "test_time_augment": false
        },
        "prefix": "tr",
        "random_annotation": {
            "drop": {
                "p": 0.0
            },
            "vibration": {
                "max_ratio": 0.0
            }
        },
        "sampler": {
            "name": "",
            "config": {}
        }
    },
    "valid": {
        "apply_bbox_filter": true,
        "confidence_filter": {
            "min_confidence": 0.4
        },
        "post_processor": {
            "name": "nms",
            "config": {
                "threshold": 0.2
            }
        },
        "prefix": "vl"
    }
}