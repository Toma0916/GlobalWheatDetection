{
    "debug": false,
    "general": {
        "output_dirname": "enhance_effdet/exp_001",
        "experiment_name": "enhance_effdet",
        "seed": 99,
        "model_save_interval": 10,
        "valid_image_save_interval": 3,
        "bbox_filter": {
            "max_bbox_size": 160000,
            "min_bbox_size": 90
        },
        "prefix": "gn",
        "train_valid_split": {
            "name": "random",
            "config": {}
        },
        "kfold": -1
    },
    "model": {
        "name": "efficient_det",
        "config": {
            "pretrained_path": "./weights/effdet/efficientdet_d5-ef44aea8.pth",
            "image_size": 512
        },
        "prefix": "md"
    },
    "train": {
        "batch_size": 4,
        "epochs": 50,
        "random_annotation": {
            "drop": {
                "p": 0.0
            },
            "vibration": {
                "max_ratio": 0.0
            }
        },
        "optimizer": {
            "name": "adamw",
            "config": {
                "lr": 0.0002
            }
        },
        "scheduler": {
            "name": "ReduceLROnPlateau",
            "config": {
                "mode": "min",
                "factor": 0.5,
                "patience": 1,
                "verbose": false,
                "threshold": 0.0001,
                "threshold_mode": "abs",
                "cooldown": 0,
                "min_lr": 1e-08,
                "eps": 1e-08
            }
        },
        "augment": {
            "test_time_augment": false,
            "prefix": "ag",
            "horizontal_flip": {
                "p": 0.0
            },
            "vertical_flip": {
                "p": 0.0
            },
            "random_rotate_90": {
                "p": 0.0
            },
            "blur": {
                "p": 0.0
            },
            "motion_blur": {
                "p": 0.0
            },
            "median_blur": {
                "p": 0.0
            },
            "gaussian_blur": {
                "p": 0.0
            },
            "optical_distortion": {
                "p": 0.0
            },
            "grid_distortion": {
                "p": 0.0
            },
            "elastic_transform": {
                "p": 0.0
            },
            "clahe": {
                "p": 0.0
            },
            "channel_shuffle": {
                "p": 0.0
            },
            "random_gamma": {
                "p": 0.0
            },
            "hsv": {
                "p": 0.0
            },
            "rgb_shift": {
                "p": 0.0
            },
            "random_brightness": {
                "p": 0.0
            },
            "random_contrast": {
                "p": 0.0
            },
            "random_sized_bbox_safe_crop": {
                "p": 0.0,
                "height": 1024,
                "width": 1024
            },
            "gauss_noise": {
                "p": 0.0
            },
            "cutout": {
                "p": 0.0
            },
            "mosaic": {
                "p": 0.0
            },
            "cutmix": {
                "p": 0.0
            },
            "mixup": {
                "p": 0.0
            }
        },
        "prefix": "tr",
        "sampler": {
            "name": "",
            "config": {}
        }
    },
    "valid": {
        "apply_bbox_filter": true,
        "confidence_filter": {
            "min_confidence": 0.2
        },
        "post_processor": {
            "name": "wbf",
            "config": {
                "threshold": 0.6
            }
        },
        "prefix": "vl"
    }
}