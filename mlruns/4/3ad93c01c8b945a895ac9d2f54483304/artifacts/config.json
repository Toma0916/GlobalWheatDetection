{
    "debug": true,
    "general": {
        "output_dirname": "koji/exp_011",
        "experiment_name": "manhattan",
        "seed": 42,
        "model_save_interval": 10,
        "bbox_filter": {
            "max_bbox_size": 160000,
            "min_bbox_size": 90
        },
        "prefix": "gn",
        "valid_image_save_interval": 5,
        "train_valid_split": {
            "name": "random",
            "config": {}
        }
    },
    "model": {
        "name": "faster_rcnn",
        "config": {
            "backbone": "fasterrcnn_resnet50_fpn",
            "class_num": 2,
            "pretrained": true
        },
        "prefix": "md"
    },
    "train": {
        "batch_size": 8,
        "epochs": 100,
        "optimizer": {
            "name": "sgd",
            "config": {
                "lr": 0.005,
                "momentum": 0.9,
                "weight_decay": 0.0005
            }
        },
        "scheduler": {
            "name": "Step",
            "config": {
                "step_size": 10,
                "gamma": 0.1,
                "last_epoch": -1
            }
        },
        "augment": {
            "horizontal_flip": {
                "p": 0.5
            },
            "vertical_flip": {
                "p": 0.5
            },
            "random_rotate_90": {
                "p": 0.5
            },
            "hsv": {
                "p": 0.4
            },
            "random_contrast": {
                "p": 0.3
            },
            "random_brightness": {
                "p": 0.3
            },
            "gauss_noise": {
                "p": 0.3
            },
            "random_gamma": {
                "p": 0.3
            },
            "clahe": {
                "p": 0.3
            },
            "test_time_augment": false,
            "prefix": "ag",
            "blur": {
                "p": 0.0
            },
            "motion_blur": {
                "p": 0.0
            },
            "median_blur": {
                "p": 0.0
            },
            "gaussian_blur": {
                "p": 0.0
            },
            "optical_distortion": {
                "p": 0.0
            },
            "grid_distortion": {
                "p": 0.0
            },
            "elastic_transform": {
                "p": 0.0
            },
            "channel_shuffle": {
                "p": 0.0
            },
            "rgb_shift": {
                "p": 0.0
            },
            "random_sized_bbox_safe_crop": {
                "p": 0.0,
                "height": 1024,
                "width": 1024
            },
            "cutout": {
                "p": 0.0
            },
            "mosaic": {
                "p": 0.0
            }
        },
        "prefix": "tr",
        "random_annotation": {
            "drop": {
                "p": 0.0
            },
            "vibration": {
                "max_ratio": 0.0
            }
        },
        "sampler": {
            "name": "",
            "config": {}
        }
    },
    "valid": {
        "prefix": "vl",
        "apply_bbox_filter": false,
        "confidence_filter": {
            "min_confidence": 0.0
        },
        "post_processor": {
            "name": ""
        }
    }
}